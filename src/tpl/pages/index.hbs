<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{{lang.wfm}}</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="css/bootstrap.css" rel="stylesheet" media="screen">
    <link href="css/bootstrap-theme.css" rel="stylesheet" media="screen">
    <link href="css/main.css" rel="stylesheet" />
    <link href="css/jquery.fileupload.css" rel="stylesheet" />
    <link href="css/jquery.Jcrop.css" rel="stylesheet" />
    <!-- date picker -->
    <link href="css/default.css" rel="stylesheet" />
    <link href="css/default.date.css" rel="stylesheet" />
    <link href="css/default.time.css" rel="stylesheet" />
    <!-- /date picker-->
</head>
<body>
    <!--[if lt IE 9]><p class="chromeframe">You are using an <strong>outdated</strong> browser. Please upgrade your browser.</p><![endif]-->
    <div id="workspace-project">
        <div class="spinner"></div>
        <div data-bind="with: vwmUserProfile">
            <nav class="wfm-nav clearfix navbar-inverse">
                <div class="pull-left">
                    <span class="link-view-inverse" data-bind="click: mdlUserProfile.loadAccountInfo">{{lang.wfm}}</span>
                </div>
                <div class="pull-right" style="display:none" data-bind="visible: mdlUserProfile.isLogged">
                    <button type="button" class="btn btn-xs btn-inverse" data-bind="click: mdlUserProfile.logOff">
                        <i class="glyphicon glyphicon-off"></i>
                        {{capitalizeFirst lang.logOut}}
                    </button>
                </div>
            </nav>
            <div class="wfm-content-wrap">
                <div>
                    <div style="display:none" data-bind="visible: mdlUserProfile.isLoadedAccountInfo">
                        <div data-bind="ifnot: mdlUserProfile.isLogged">
                            <div data-bind="if: isRegistered">
                                <div data-bind="with: mdlUserProfile">
                                    {{> account-logon }}
                                </div>
                            </div>
                            <div data-bind="ifnot: isRegistered">
                                <div data-bind="with: mdlUserProfile">
                                    {{> account-register }}
                                </div>
                            </div>
                        </div>
                        <div data-bind="visible: mdlUserProfile.isLogged">
                            <div data-bind="ifnot: mdlUserProfile.isLoadedEmployees">{{lang.loading}}</div>
                            <div style="display:none" data-bind="visible: mdlUserProfile.isLoadedEmployees">
                                <!-- If no selected employee -->
                                <div data-bind="ifnot: vwmEmployee">
                                    <div data-bind="with: mdlUserProfile">
                                        {{> employee-list }}
                                    </div>
                                </div>
                                <div data-bind="with: vwmEmployee">
                                    <div data-bind="if: mdlEmployee.canEditAll">
                                        {{> edit-mode-btn }}
                                    </div>
                                    <div>
                                        <div>
                                            <div class="clearfix">
                                                <div class="sidebar-toggle" data-bind="css: $root.sidebarToggleCss">
                                                    <button type="button" data-bind="click: $root.toggleIsVisibleMenu" title="Hide/show menu">
                                                        <i class="glyphicon glyphicon-chevron-left" data-bind="visible: $root.isVisibleMenu"></i>
                                                        <i class="glyphicon glyphicon-chevron-right" data-bind="hidden: $root.isVisibleMenu"></i>
                                                    </button>
                                                </div>
                                                <div class="sidebar-wrap" data-bind="css: $root.sidebarWrapCss">
                                                    <div data-bind="scroller: { }">
                                                        {{> workspace-menu }}
                                                    </div>
                                                </div>
                                                <div class="workspace-container" data-bind="with: vwmCompany, css: $root.workAreaCss">
                                                    <div class="wfm-top-tab-panel file-mgr-block">
                                                        <button class="btn btn-inverse btn-sm" type="button" data-bind="click: mdlCompany.modalFileMgr.show">
                                                            <i class="glyphicon glyphicon-folder-open"></i>&nbsp;&nbsp;<span>{{capitalizeFirst lang.fileManager}}</span>
                                                        </button>
                                                    </div>
                                                    {{> modal-file-mgr }}
                                                    <div data-bind="ifnot: vwmWegion">
                                                        <div data-bind="with: mdlCompany">
                                                            {{> section-panel-standard }}
                                                            {{> dashboard-block }}
                                                            {{> company }}
                                                        </div>
                                                    </div>
                                                    <div data-bind="with: vwmWegion">
                                                        <div data-bind="ifnot: vwmWield">
                                                            <div data-bind="with: mdlWegion">
                                                                {{> section-panel-standard }}
                                                                {{> dashboard-block }}
                                                                {{> wegion }}
                                                            </div>
                                                        </div>
                                                        <div data-bind="with: vwmWield">
                                                            <div data-bind="ifnot: vwmWroup">
                                                                <div data-bind="with: mdlWield">
                                                                    {{> section-panel-standard }}
                                                                    {{> dashboard-block }}
                                                                </div>
																{{> wield }}
                                                            </div>
                                                            <div data-bind="with: vwmWroup">
                                                                <div data-bind="with: mdlWroup">
                                                                    <div data-bind="ifnot: selectedWell">
                                                                        {{> section-panel-standard }}
                                                                        {{> dashboard-block }}
                                                                        {{> wroup }}
                                                                    </div>
                                                                    <div data-bind="with: selectedWell">
                                                                        {{> section-panel-well }}
                                                                        {{> dashboard-block }}
                                                                        {{> well }}
                                                                    </div>
                                                                </div>
                                                            </div>

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- end: isLoadedAccountInfo -->
                </div>
            </div>
        </div>
        {{> modal-standard }}
        {{> modal-panzoom }}
        {{> modal-wide }}
        {{> clip-path-perfomance }}
    </div>
    <footer class="navbar-fixed-bottom navbar-inverse">
        <div class="footer-logo-wrap">
            <div class="footer-logo"></div>
        </div>
    </footer>
    <!-- Load main script file for page or default (with auth logic only) -->
    {{#if conf.isProd}}
    <script data-main="js/main-bundle-{{package.version}}" src="js/require.js"></script>
    {{else}}
    <script data-main="js/main" src="js/require.js"></script>
    {{/if}}
</body>
</html>