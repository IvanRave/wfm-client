<link href="#" id="wfm-style-link" rel="stylesheet" />
<script>
  (function(){
    var styleLinkElem = document.getElementById('wfm-style-link');
    var cssPathCookie = window.localStorage.getItem('{{ syst.wfmStyleLinkCookie }}');
    styleLinkElem.href = (cssPathCookie || 'css/main-light.css') + '?{{package.version}}';
  })();
</script>