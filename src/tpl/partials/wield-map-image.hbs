<svg class="wield-map-svg" version="1.1" width="100%" viewbox="0 0 1200 600"
     data-bind="attr: { height: mapWrap.height }, svgResponsive: { koSvgWidth: mapWrap.width, tmpIsVisibleMenu: $root.isVisibleMenu}">
  <rect width="1200" height="600"></rect>
  <g data-bind="svgMap: {imgUrl: mdlMapOfWield.fileSpec.fileUrl, imgSizePx: imgSizePx, listOfVwmWellMarker: listVwmWellMarker, wellMarkerRadius: 8, svgBlockSize: svgBlockSize, idOfSlcMapTool: idOfSlcVwmMapTool, slcVwmWellMarker: slcVwmWellMarker, transformAttr: transformAttr, wellMarkerDataToAdd: wellMarkerDataToAdd}">
    <g>
      <!--inner group for transforming and scale group of next elements: image, well markers and areas-->
    </g>
  </g>
</svg>
<span data-bind="with: transformAttr">
  scale: <span data-bind="text: scale"></span><br/>
  translate<span data-bind="text: translate"></span>
</span>